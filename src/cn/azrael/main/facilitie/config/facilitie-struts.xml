<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
          "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="facilitie-action" namespace="/facilitie" extends="json-default">
    	
    	<action name="facilitie_*" class="cn.azrael.main.facilitie.action.FacilitieAction" method="{1}">
    		<result name="default">/WEB-INF/jsp/facilitie/facilitie_main.jsp</result>
    		<result name="list" type="redirectAction">
    			<param name="actionName">facilitie_info</param>
    			<param name="pageNo">${pageNo}</param>
    		</result>
    		<result name="{1}">/WEB-INF/jsp/facilitie/facilitie_{1}.jsp</result>
    	</action>
    	<action name="facilitiefix_*" class="cn.azrael.main.facilitie.action.FacilitieFixAction" method="{1}">
    		<result name="list" type="redirectAction">
    			<param name="actionName">facilitiefix_info</param>
    			<param name="pageNo">${pageNo}</param>
    		</result>
    		<result name="{1}">/WEB-INF/jsp/facilitie/facilitiefix_{1}.jsp</result>
    	</action>
    </package>
</struts>
