<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.azrael.main.facilitie.entity">
	<class name="Facilitie" table="facilities">
		<id name="id" type="string">
			<column name="f_id" length="32"></column>
			<generator class="uuid.hex"></generator>
		</id>
		<property name="name" column="f_name" type="string" length="20" not-null="true"></property>
		<property name="type" column="f_type" type="integer"  not-null="true"></property>
		<property name="normalNum" column="f_n_num" type="integer"  not-null="true"></property>
		<property name="badNum" column="f_b_num" type="integer" not-null="true"></property>
	</class>

</hibernate-mapping>



