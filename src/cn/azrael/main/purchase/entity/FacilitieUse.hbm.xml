<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.azrael.main.purchase.entity">
	<class name="FacilitieUse" table="facilitie_use">
		<id name="id" type="string">
			<column name="fu_id" length="32"></column>
			<generator class="uuid.hex"></generator>
		</id>
		<property name="useNum" column="fu_num" type="integer" not-null="true"></property>
		<property name="date" column="fu_date" type="double" not-null="true"></property>
		<many-to-one name="facilitieId" class="cn.azrael.main.facilitie.entity.Facilitie" column="f_id" not-null="true"></many-to-one>
	</class>

</hibernate-mapping>



