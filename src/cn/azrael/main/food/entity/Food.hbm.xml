<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.azrael.main.food.entity">
	<class name="Food" table="food">
		<id name="id" type="string">
			<column name="fo_id" length="32"></column>
			<generator class="uuid.hex"></generator>
		</id>
		<property name="name" column="fo_name" type="string" not-null="true"></property>
		<property name="type" column="fo_type" type="integer"  not-null="true"></property>
		<property name="status" column="fo_status" type="boolean"  not-null="true"></property>
		<property name="price" column="fo_price" type="double"  not-null="true"></property>
		<property name="style" column="fo_style" type="string"  not-null="true"></property>
	</class>

</hibernate-mapping>



