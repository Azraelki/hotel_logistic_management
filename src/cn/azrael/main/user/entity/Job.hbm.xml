<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.azrael.main.user.entity">
	<class name="Job" table="jobs">
		<id name="id" type="integer">
			<column name="j_id"></column>
			<generator class="assigned"></generator>
		</id>
		<property name="name" column="j_name" type="string" length="20" not-null="true"></property>
		<property name="task" column="j_task" type="string" length="20" not-null="true"></property>
	</class>

</hibernate-mapping>
