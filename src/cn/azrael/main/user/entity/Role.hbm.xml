<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.azrael.main.user.entity">
	<class name="Role" table="role">
		<id name="id" type="string">
			<column name="r_id" length="32"></column>
			<generator class="uuid.hex"></generator>
		</id>
		<property name="type" column="r_type" type="integer"  not-null="true"></property>
		<property name="privilege" column="privilege" type="string"  length="10" not-null="true"></property>
	</class>

</hibernate-mapping>



