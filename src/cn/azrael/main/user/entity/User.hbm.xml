<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.azrael.main.user.entity">
	<class name="User" table="users">
		<id name="id" type="string">
			<column name="u_id" length="32"></column>
			<generator class="uuid.hex"></generator>
		</id>
		<property name="type" column="u_type" type="integer"  not-null="true"></property>
		<property name="password" column="u_password" type="string"  length="20" not-null="true"></property>
		<many-to-one name="employeeId"   class="Employee" column="e_id" unique="true" not-null="true"></many-to-one>
	</class>

</hibernate-mapping>



