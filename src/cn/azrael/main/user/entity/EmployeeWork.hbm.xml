<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.azrael.main.user.entity">
	<class name="EmployeeWork" table="employee_works">
		<id name="id" type="string">
			<column name="ew_id" length="32"></column>
			<generator class="uuid.hex"></generator>
		</id>
		<property name="leaveNum" column="ew_leave" type="integer"  not-null="true"></property>
		<property name="cleanNum" column="ew_clean" type="integer"  not-null="true"></property>
		<property name="date" column="ew_date" type="double"   not-null="true"></property>
		<many-to-one name="employeeId" class="Employee" column="e_id" not-null="true"></many-to-one>
	</class>

</hibernate-mapping>



