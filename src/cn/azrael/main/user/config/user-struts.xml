<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
          "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="user-action" namespace="/user" extends="struts-default">
    	<action name="employee_*" class="cn.azrael.main.user.action.EmployeeAction" method="{1}">
    		<result name="default">/WEB-INF/jsp/user/user_main.jsp</result>
    		<result name="list" type="redirectAction">
    			<param name="actionName">employee_info</param>
    			<param name="pageNo">${pageNo}</param>
    		</result>
    		<result name="{1}">/WEB-INF/jsp/user/user_{1}.jsp</result>
    	</action>
    	<action name="job_*" class="cn.azrael.main.user.action.JobAction" method="{1}">
    		<result name="{1}">/WEB-INF/jsp/user/job_{1}.jsp</result>
    	</action>
    	<action name="work_*" class="cn.azrael.main.user.action.WorkAction" method="{1}">
    		<result name="{1}">/WEB-INF/jsp/user/work_{1}.jsp</result>
    	</action>
    </package>
</struts>
