<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.azrael.main.linen.entity">
	<class name="LinensInfo" table="linens_info">
		<id name="id" type="string">
			<column name="li_id" length="32"></column>
			<generator class="uuid.hex"></generator>
		</id>
		<property name="recNum" column="li_rec_num" type="integer" not-null="true"></property>
		<property name="senNum" column="li_sen_num" type="integer"  not-null="true"></property>
		<property name="backWashNum" column="li_back_wash_num" type="integer"  not-null="true"></property>
		<property name="oweNum" column="li_owe_num" type="integer" not-null="true"></property>
		<many-to-one name="linenId" class="Linen" column="l_id" not-null="true"></many-to-one>
		<many-to-one name="facilitieId" class="cn.azrael.main.facilitie.entity.Facilitie" column="f_id" not-null="true"></many-to-one>
	</class>

</hibernate-mapping>



